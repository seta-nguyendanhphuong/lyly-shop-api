{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "productStatus": {
      "type": "boolean",
      "required": true
    },
    "productImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "productDescription": {
      "type": "text"
    },
    "productRentalPrice": {
      "type": "decimal"
    },
    "productRetailPrice": {
      "type": "decimal"
    }
  }
}
